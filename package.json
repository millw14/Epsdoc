{
  "name": "webstein",
  "version": "1.0.0",
  "description": "Webstein - Interactive document network explorer",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node --loader tsx api_server.ts",
    "dev": "tsx api_server.ts",
    "build": "cd network-ui && npm install && npm run build",
    "build:frontend": "cd network-ui && npm run build",
    "postinstall": "cd network-ui && npm install && npm run build",
    "analyze": "tsx analyze_documents.ts",
    "query": "tsx query_db.ts",
    "review": "tsx review.ts",
    "split": "tsx split_large_docs.ts",
    "api": "tsx api_server.ts",
    "dedupe": "tsx dedupe_actors.ts",
    "dedupe-auto": "tsx dedupe_actors_auto.ts",
    "dedupe-llm": "tsx dedupe_with_llm.ts"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.37",
    "@anthropic-ai/sdk": "^0.68.0",
    "@huggingface/transformers": "^3.7.6",
    "@openrouter/sdk": "^0.1.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "better-sqlite3": "^11.7.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}
